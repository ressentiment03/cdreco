<%= render 'public/homes/header' %>
<div>
  <h3>商品一覧</h3>
  <div>
    <% @items.each do |item| %>
      <%= attachment_image_tag item, :image, :fill, 200, 150 %>
      <p><%= link_to item.name, item_path(item) %></p>
      <% tax=item.price*(1+0.1) %>
      <p>¥<%= tax.round %></p>
    <% end %>
  </div>
</div>