<%= render 'public/homes/header' %>
<div>
    <h2>マイページ</h2>
    <p>登録情報</p>
    <%= link_to '編集する', edit_end_user_path %>
    <table>
        <tbody>
            <tr>
                <th>氏名</th>
                <td>
                    <%= current_end_user.last_name %>
                    <%= current_end_user.first_name %>
                </td>
            </tr>
            <tr>
                <th>カナ</th>
                <td>
                    <%= current_end_user.last_name_kana %>
                    <%= current_end_user.first_name_kana %>
                </td>
            </tr>
            <tr>
                <th>郵便番号</th>
                <td><%= current_end_user.postal_code %></td>
            </tr>
            <tr>
                <th>住所</th>
                <td><%= current_end_user.address %></td>
            </tr>
            <tr>
                <th>電話番号</th>
                <td><%= current_end_user.telephone_number %></td>
            </tr>
            <tr>
                <th>メールアドレス</th>
                <td><%= current_end_user.email %></td>
            </tr>
        </tbody>
    </table>
    
    <h3>配送先<%= link_to '一覧を見る', end_user_path %></h3>
    <h3>注文履歴<%= link_to '一覧を見る', end_user_path %></h3>
</div>